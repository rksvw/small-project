{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug index.html in Firefox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:5500/html/index.html",
      "webRoot": "${workspaceFolder}",
      "reAttach": true,
      "pathMappings": [
        {
          "url": "http://localhost:5500",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}
